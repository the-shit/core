{
    "name": "the-shit/core",
    "description": "THE SHIT - Scaling Humans Into Tomorrow",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "shit", "tight"],
    "homepage": "https://github.com/S-H-I-T/core",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/S-H-I-T/core/issues",
        "source": "https://github.com/S-H-I-T/core"
    },
    "authors": [
        {
            "name": "Jordan Partridge",
            "email": "jordan@partridge.rocks"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel-zero/framework": "^12.0",
        "jordanpartridge/conduit-interfaces": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
